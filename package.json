{
  "name": "",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "tsx ./server/index.ts",
    "start:server": "node ./dist/server-index.js",
    "build": "run-p build:*",
    "build:rr": "react-router build",
    "build:esbuild": "tsx ./esbuild.config.ts",
    "clean": "run-p clean:*",
    "clean:rr": "rm -rf ./build",
    "clean:dist": "rm -rf ./dist",
    "typecheck": "react-router typegen && tsc",
    "typesync:run": "typesync",
    "typesync:ci": "typesync --dry=fail",
    "lint:fix": "biome check --write .",
    "lint:ci": "biome ci .",
    "test": "run-s test:clean test:setup test:run",
    "test:clean": "rm -f libsql/test.db",
    "test:setup": "NODE_ENV=test DB_URL=file:libsql/test.db tsx tasks/db.ts up",
    "test:run": "NODE_ENV=test DB_URL=file:libsql/test.db vitest run"
  },
